{
  "name": "webkernel/dependencies",
  "description": "Centralized dependency bundle for the Webkernel ecosystem, offering a unified set of core libraries shared across all modules.",
  "type": "library",
  "license": "MPL-2.0",
  "authors": [
    {
      "name": "Numerimondes",
      "email": "webkernel@numerimondes.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "calebporzio/sushi": "^2.5",
    "filament/filament": "^4.0",
    "jeremykendall/php-domain-parser": "^6.4",
    "laravel/framework": "^12.0",
    "laravel/octane": "^2.12",
    "laravel/prompts": "^0.3.8",
    "laravel/tinker": "^2.10.1"
  },  
  "require-dev": {
    "phpunit/phpunit": "^12.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  }
}
